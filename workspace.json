{
  "version": 1,
  "projects": {
    "lib-a": {
      "root": "libs/lib-a",
      "sourceRoot": "libs/lib-a/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "libs/lib-a/.eslintrc",
            "tsConfig": [
              "libs/lib-a/tsconfig.lib.json",
              "libs/lib-a/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!libs/lib-a/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/lib-a/jest.config.js",
            "tsConfig": "libs/lib-a/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "lib-b": {
      "root": "libs/lib-b",
      "sourceRoot": "libs/lib-b/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "libs/lib-b/.eslintrc",
            "tsConfig": [
              "libs/lib-b/tsconfig.lib.json",
              "libs/lib-b/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!libs/lib-b/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/lib-b/jest.config.js",
            "tsConfig": "libs/lib-b/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "lib-c": {
      "root": "libs/lib-c",
      "sourceRoot": "libs/lib-c/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "libs/lib-c/.eslintrc",
            "tsConfig": [
              "libs/lib-c/tsconfig.lib.json",
              "libs/lib-c/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!libs/lib-c/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/lib-c/jest.config.js",
            "tsConfig": "libs/lib-c/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "app": {
      "root": "apps/app",
      "sourceRoot": "apps/app/src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/app",
            "main": "apps/app/src/main.ts",
            "tsConfig": "apps/app/tsconfig.app.json",
            "assets": [
              "apps/app/src/assets"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/app/src/environments/environment.ts",
                  "with": "apps/app/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "app:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/app/.eslintrc",
            "tsConfig": [
              "apps/app/tsconfig.app.json",
              "apps/app/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/app/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/app/jest.config.js",
            "tsConfig": "apps/app/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/node"
  },
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "eslint"
      }
    },
    "@nrwl/react": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/next": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/web": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/nx-plugin": {
      "plugin": {
        "linter": "eslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "app"
}
